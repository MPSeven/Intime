<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:overScrollMode="never"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_child"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_child"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_childrv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="30dp"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    tools:listitem="@layout/list_checklist" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#4DEDEDED" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_women"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_women"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_womenrv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    android:layout_marginBottom="30dp"
                    tools:listitem="@layout/list_checklist"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#4DEDEDED" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_medicine"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_medicine"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_medicinerv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    android:layout_marginBottom="30dp"
                    tools:listitem="@layout/list_checklist"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#4DEDEDED" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_pet"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_pet"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_petrv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    android:layout_marginBottom="30dp"
                    tools:listitem="@layout/list_checklist"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#4DEDEDED" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_valuables"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_valuables"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_valuablesrv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    android:layout_marginBottom="30dp"
                    tools:listitem="@layout/list_checklist"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#4DEDEDED" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_clothing"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_clothing"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_clothingrv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    android:layout_marginBottom="30dp"
                    tools:listitem="@layout/list_checklist"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="5dp"
                android:background="#4DEDEDED" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/ch_extra_etc"
                    android:layout_width="match_parent"
                    android:layout_height="22dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="24dp"
                    android:text="@string/info_chk_etc"
                    android:textAppearance="@style/Head3"
                    android:textFontWeight="1100"
                    android:fontFamily="@font/spoqa_han_sans_neo_regular"/>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/ch_extra_etcrv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:overScrollMode="never"
                    android:fillViewport="true"
                    android:layout_marginBottom="30dp"
                    tools:listitem="@layout/list_checklist"/>
            </LinearLayout>

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>
